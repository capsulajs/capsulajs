{
  "name": "@capsulajs/capsulajs",
  "version": "0.0.1",
  "description": "A set of utility tool to manage CapsulaJS environment",
  "main": "index.js",
  "repository": "git@github.com:capsulajs/capsulajs.git",
  "author": "CapsulaJS",
  "license": "MIT",
  "private": true,
  "keywords": [
    "capsulajs"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --stream --parallel",
    "lint": "lerna run lint --stream --parallel",
    "test": "lerna run test --stream --parallel",
    "bootstrap": "lerna bootstrap"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lerna": "^3.16.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "bugs": {
    "url": "https://github.com/capsulajs/capsulajs/issues"
  },
  "homepage": "https://github.com/capsulajs/capsulajs"
}
